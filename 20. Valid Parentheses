// SOLUTION 1(STRING)
class Solution {
    public boolean isValid(String s) {
        while(s.contains("()") || s.contains("{}") || s.contains("[]")){
            s = s.replace("()","");
            s = s.replace("{}","");
            s = s.replace("[]","");
        }
        return s.length() == 0;
    }
}
//SOLUTION 2(STACK)
class Solution {
    public boolean isValid(String s) {
        Stack <Character> stk = new Stack<>();
        for(char c: s.toCharArray()){
            if(c == '('){
                stk.push(')');
            }else if(c == '{'){
                stk.push('}');
            }else if(c == '['){
                stk.push(']');
            }else if(stk.isEmpty() || stk.pop() != c){
                return false;
            }
        }
        if(stk.size() == 0) return true;
        return false;
    }
}

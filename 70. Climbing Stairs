//SOLUTUON 1
class Solution {
    public int climbStairs(int n) {
        if(n==1 || n==0){
            return 1;
        }
        int[] dp = new int[n+1];
        dp[1]=1;
        dp[2]=2;
        for(int i=3;i<n+1;i++){
            dp[i] = dp[i-1]+dp[i-2];
        }
        return dp[n];

    }
}
// SOLUTION 2
class Solution {
    public int climbStairs(int n) {
        int a=0;
        int b=1;
        for(int i=0;i<n;i++){
            int c=a+b;
            a=b;
            b=c;
        }
        return b;
    }
}

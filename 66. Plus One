class Solution {
    public int[] plusOne(int[] digits) {
        Scanner sc = new Scanner(System.in);
        int n = digits.length;
        for(int i=n;i>0;i--){
            if(digits[i]<9){
                digits[i]+=1;
                return digits;
            }
            else{
                digits[i]=0;
            }
        } 
        int[] arr = new int[n+1];
        arr[0]=1;
        return arr;
    }
}
